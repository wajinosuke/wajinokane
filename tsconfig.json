// このファイルはコンパイルに利用されないが、vscode用に設定ファイルを残す
{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es5", // TSはECMAScript 5に変換
    "module": "es2015", // TSのモジュールはES Modulesとして出力
    "moduleResolution": "node", //　各モジュール解決のために設定
    "lib": [
      "es2015",
      "es2015.promise",
      "dom"
    ],
    "jsx": "react"
  }
}